#include <iostream>
using namespace std;


  private:
    float celsius;

  
  public:
    
    void getTemperature() {
      cout << "Enter Temperature in Celsius:";
      cin >> celsius;
    }

  

    fahrenheit = ((celsius * 9) / 5) + 32;

   
    return fahrenheit;
  }
};

int main() {
  
  float temperature;

 
  C.getTemperature();

  

  cout << "Temperature in Fahrenheit :" << temperature;

  return 0;
}
